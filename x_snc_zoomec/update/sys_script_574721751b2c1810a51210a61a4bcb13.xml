        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var keywordList = '';
	var keywords = new GlideRecord('x_snc_zoomec_transcript_keywords');
	keywords.addEncodedQuery('conference_call=' + current.u_conference_call);
	keywords.query();
	
	while(keywords.next()) {
		if (keywords.keyword != '') {
			keywordList += ',' + keywords.keyword;
		}
	}

	arr = gs.getProperty('x_snc_zoomec.TableLabels') + ',' + gs.getProperty('x_snc_zoomec.PrefixList') + keywordList;
	arr = arr.split(',');

	var extraCrispy = new crispyZoom();
	extraCrispy.createMeetingMentions(arr,current.getUniqueValue(),current.u_conference_call);

})(current, previous);]]></script>
